{
  "name": "spotscan",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "gh-pages -d dist",
    "dev": "vite --mode dev",
    "dev:https": "cross-env HTTPS=true vite --mode dev",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode prod",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --report-unused-disable-directives --max-warnings 0 --fix"
  },
  "dependencies": {
    "@telegram-apps/sdk-vue": "2.0.31",
    "@ton/core": "^0.61.0",
    "@tonconnect/ui": "^2.0.9",
    "@types/numeral": "^2.0.5",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "clipboardy": "^4.0.0",
    "eruda": "^3.4.1",
    "numeral": "^2.0.6",
    "pinia": "^3.0.3",
    "vant": "^4.9.21",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vue": "^3.5.12",
    "vue-input-autowidth": "^2.2.1",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "gh-pages": "^6.2.0",
    "npm-run-all2": "^7.0.1",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.1.10"
  },
  "engines": {
    "node": "^22"
  }
